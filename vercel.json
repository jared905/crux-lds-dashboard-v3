{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/sync-competitors",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/daily-sync",
      "schedule": "0 7 * * *"
    }
  ]
}
